}


Diagnostic
{
	title = Average Potential
	A1 = x1min
	A2 = x2ANOmin
	B1 = x1ANOmax
	B2 = x2ANOmax
	VarName = phi
	x1_Label = time
  	x2_Label = Voltage
	integral = sum
  	HistMax = 10240
}

Diagnostic
{
	title = Wave detection near entrance
	A1 = 0.25*x1max
	B1 = 0.25*x1max
	A2 = x2max/2  //0
	B2 = x2max/2
	VarName = phi
	x1_Label = time
  	x2_Label = Voltage
	integral = sum
  	HistMax = 10240
}
Diagnostic
{
	title = Wave detection near CATHODE
	A1 = 0.95*x1max
	B1 = 0.95*x1max
	A2 = x2max/2  //0
	B2 = x2max/2
	VarName = phi
	x1_Label = time
  	x2_Label = Voltage
	integral = sum
