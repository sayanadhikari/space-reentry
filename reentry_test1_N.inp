	A1 = 0
	A2 = 0
	B1 = 0
	B2 = x2max
	normal = 1
	speciesName = Nitrogen
	I = Ji*x2max
	np2c = np2c
	v1drift = 1E2
	v2drift = 0
	v3drift = 0
