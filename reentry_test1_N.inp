//	v2thermal = 0
//	v3thermal = 0
	//Ucutoff = VefCUTOFF
   units = EV
   temperature =  0.01 //6.5655e+04
}

BeamEmitter
{
	A1 = 0
	A2 = 0
	B1 = 0
	B2 = x2max
	normal = 1
	speciesName = electrons
	I = Je*x2max
	np2c = np2c
	v1drift = 1E2
	v2drift = 0
	v3drift = 0
	//Ucutoff = VefCUTOFF
//	v1thermal = 10
//	v2thermal = 0
//	v3thermal = 0
   units = EV
   temperature = 0.1	//3.9785e+06
